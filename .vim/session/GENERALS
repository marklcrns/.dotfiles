let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd /mnt/c/Users/markl/Projects/java/eclipse-workspace/Generals-2.0
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +499 core/src/main/java/com/markl/game/engine/board/Move.java
badd +43 core/src/test/java/com/markl/game/engine/board/MoveTest.java
badd +1 core/src/main/java/com/markl/game/Generals.java
argglobal
%argdel
edit core/src/main/java/com/markl/game/Generals.java
set splitbelow splitright
wincmd t
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
argglobal
setlocal fdm=syntax
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=99
setlocal fml=1
setlocal fdn=20
setlocal fen
let s:l = 24 - ((23 * winheight(0) + 18) / 36)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
24
normal! 0
lcd /mnt/c/Users/markl/Projects/java/eclipse-workspace/Generals-2.0
